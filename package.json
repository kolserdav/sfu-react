{
  "name": "uyem",
  "version": "0.7.5",
  "repository": "https://github.com/kolserdav/werift-sfu-react.git",
  "description": "Selective Forwarding Unit (SFU) is a WebRTC web application library, the server part of which is written in Node, and the client part is a ReactJS/Next.js shared component.",
  "keywords": [
    "node",
    "react",
    "next",
    "webrtc",
    "p2p",
    "peer",
    "group",
    "video",
    "call",
    "chat"
  ],
  "author": {
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com"
  },
  "maintainers": [
    "Sergey Kolmiller <uyem.ru@gmail.com>"
  ],
  "contributors": [
    "Sergey Kolmiller <uyem.ru@gmail.com>"
  ],
  "bugs": "https://github.com/kolserdav/werift-sfu-react/issues",
  "license": "MIT",
  "main": "packages/client/dist/Main.js",
  "types": "packages/client/dist/Main.d.ts",
  "files": [
    "packages/client/dist",
    "packages/server/dist",
    "bin",
    "server.js",
    "server.d.ts",
    "packages/server/orm",
    "packages/server/package.json",
    "scripts"
  ],
  "bin": {
    "uyem": "bin/uyem.js"
  },
  "prisma": {
    "schema": "packages/server/orm/schema.prisma"
  },
  "scripts": {
    "build:server": "cd packages/server && npm run build",
    "clean:server": "cd packages/server && npm run clean",
    "clean": "npm run clean:server",
    "deploy": "npm run clean && npm run build",
    "prebuild:client": "cd packages/client && npm run build",
    "build:client": "cd packages/client && npm run compile",
    "start:client": "cd packages/client && npm run start",
    "build": "npm run build:server && npm run build:client",
    "copy-interfaces": "node scripts/create-interfaces-symlink.js",
    "copy": "npm run copy-interfaces",
    "dev:migrate": "cd packages/server && npm run dev:migrate",
    "dev:server": "cd packages/server && npm run dev",
    "dev:client": "cd packages/client && npm run dev",
    "env:server": "cp packages/server/.env.example packages/server/.env",
    "env:client": "cp packages/client/.env.example packages/client/.env",
    "env": "npm run env:server && npm run env:client",
    "postinstall": "npm run copy",
    "prod:migrate": "cd packages/server && npm run migrate",
    "start:server": "cd packages/server && npm run start",
    "start": "npm run start:server",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test": "node tests/rooms.js",
    "test:browser": "node tests/webrtc-browser.js"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.1",
    "prisma": "^4.2.1",
    "puppeteer": "^15.3.2",
    "puppeteer-screen-recorder": "^2.0.2",
    "react-cookie": "^4.1.1",
    "remove": "^0.1.5",
    "uuid": "^8.3.2",
    "uyem": "^0.6.6",
    "webrtc-adapter": "^8.1.1",
    "werift": "^0.15.11",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@types/webrtc": "^0.0.32",
    "@types/ws": "^8.5.3",
    "react-scripts": "^5.0.1"
  },
  "optionalDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "18.7.13",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "airbnb": "^0.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "next": "12.2.5",
    "nodemon": "^2.0.19",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "fhead": {
    "root": "packages",
    "repository": "https://github.com/kolserdav/werift-sfu-react.git",
    "patterns": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "werift-webrtc",
      "dist",
      "build"
    ],
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com",
    "license": "MIT",
    "licenseText": "See in LICENSE file",
    "copyright": "kolserdav, All rights reserved (c)",
    "renewAll": false
  }
}
